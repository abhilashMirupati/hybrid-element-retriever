{
  "intents": [
    {
      "id": "iframe_1",
      "description": "Enter full name in billing",
      "step": "Type 'John Smith' in the Full Name field",
      "expected_element": {
        "type": "input[type=text]",
        "attributes": ["fullName", "Full Name"],
        "context": "billing information section"
      }
    },
    {
      "id": "iframe_2",
      "description": "Enter card number in payment iframe",
      "step": "Enter credit card number in the payment form",
      "expected_element": {
        "type": "input",
        "attributes": ["cardNumber", "1234 5678 9012 3456"],
        "context": "inside payment iframe"
      }
    },
    {
      "id": "iframe_3",
      "description": "Enter CVV in payment iframe",
      "step": "Type CVV security code",
      "expected_element": {
        "type": "input",
        "attributes": ["cvv", "123"],
        "context": "payment iframe, next to expiry date"
      }
    },
    {
      "id": "iframe_4",
      "description": "Validate payment card",
      "step": "Click Validate Card button in the payment form",
      "expected_element": {
        "type": "button",
        "text": "Validate Card",
        "attributes": ["validate-btn"],
        "context": "inside payment iframe"
      }
    },
    {
      "id": "iframe_5",
      "description": "Select express shipping",
      "step": "Choose Express Shipping option",
      "expected_element": {
        "type": "radio",
        "attributes": ["shipping", "express"],
        "context": "shipping iframe, $15.99 option"
      }
    },
    {
      "id": "iframe_6",
      "description": "Select overnight shipping",
      "step": "Select Overnight Shipping for next day delivery",
      "expected_element": {
        "type": "radio",
        "attributes": ["shipping", "overnight"],
        "context": "shipping iframe, $29.99 option"
      }
    },
    {
      "id": "iframe_7",
      "description": "Complete order",
      "step": "Click Complete Order button to finalize purchase",
      "expected_element": {
        "type": "button",
        "text": "Complete Order",
        "attributes": ["submitOrder", "submit-btn"],
        "context": "bottom of checkout form"
      }
    },
    {
      "id": "iframe_8",
      "description": "Enter cardholder name",
      "step": "Type cardholder name in the payment form",
      "expected_element": {
        "type": "input[type=text]",
        "attributes": ["cardName", "John Doe"],
        "context": "payment iframe, below CVV field"
      }
    },
    {
      "id": "iframe_9",
      "description": "Select country",
      "step": "Choose United States from the country dropdown",
      "expected_element": {
        "type": "select",
        "attributes": ["country", "Country"],
        "context": "billing information section"
      }
    }
  ]
}