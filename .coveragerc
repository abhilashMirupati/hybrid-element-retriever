[run]
branch = True
source =
    src/her
omit =
    src/her/actions.py
    src/her/executor/*
    src/her/gateway_server.py
    src/her/handlers/*
    src/her/production_client.py
    src/her/resilience.py
    src/her/embeddings/enhanced_*.py
    src/her/embeddings/fallback_embedder.py
    src/her/embeddings/minilm_embedder.py
    src/her/embeddings/real_embedder.py
    src/her/embeddings/text_embedder.py
    src/her/locator/enhanced_verify.py
    src/her/session/enhanced_manager.py
    src/her/session/snapshot.py
    src/her/bridge/*
    src/her/vectordb/*.py
    src/her/descriptors/*.py
    src/her/mock_client.py
    src/her/cli.py
    src/her/cli_api.py
    src/her/utils/*.py
    src/her/validators.py
    src/her/recovery/*.py

[report]
omit =
    
include =
    */her/compat.py
    */her/cache/two_tier.py
    */her/embeddings/_resolve.py
    */her/embeddings/element_embedder.py
    */her/embeddings/query_embedder.py
    */her/rank/fusion.py

precision = 0
skip_covered = False
show_missing = True
